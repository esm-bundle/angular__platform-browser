{
  "name": "@esm-bundle/angular__platform-browser",
  "version": "14.2.0",
  "description": "ESM and SystemJS versions of @angular/platform-browser",
  "type": "module",
  "exports": {
    ".": {
      "es2015": "./es2015/ivy/angular-platform-browser.js",
      "es2020": "./es2020/ivy/angular-platform-browser.js",
      "default": "./es2020/ivy/angular-platform-browser.js"
    },
    "./animations": {
      "es2015": "./es2015/ivy/angular-animations.js",
      "es2020": "./es2020/ivy/angular-animations.js",
      "default": "./es2020/ivy/angular-animations.js"
    }
  },
  "scripts": {
    "test": "pnpm run build && concurrently -n w: 'pnpm:test:*'",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "debug:test:browser": "karma start karma.conf.cjs",
    "test:formatting": "prettier --check .",
    "build": "rimraf system es2015 es2020 && rollup -c",
    "format": "prettier --write .",
    "release": "release-it",
    "prepublishOnly": "pnpm build && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "files": [
    "es2015",
    "es2020",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/angular__platform-browser.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esm-bundle/angular__platform-browser/issues"
  },
  "homepage": "https://github.com/esm-bundle/angular__platform-browser#readme",
  "devDependencies": {
    "@angular/common": "13.3.1",
    "@angular/compiler": "14.2.0",
    "@angular/compiler-cli": "14.2.0",
    "@angular/core": "13.3.1",
    "@angular/platform-browser": "14.2.0",
    "@rollup/plugin-babel": "5.3.1",
    "concurrently": "7.3.0",
    "esm-bundle-scripts": "1.2.0",
    "husky": "8.0.1",
    "karma": "6.4.0",
    "karma-firefox-launcher": "2.1.2",
    "karma-jasmine": "5.1.0",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "release-it": "15.4.0",
    "release-it-plugin-esm-bundle": "3.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.0",
    "rollup-plugin-terser": "7.0.2",
    "rxjs": "7.5.6",
    "typescript": "4.8.2"
  }
}
